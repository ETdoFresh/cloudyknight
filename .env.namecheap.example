# Namecheap Domain Configuration
DOMAIN=workspaces.etdofresh.com

# Namecheap Dynamic DNS (for update script)
NAMECHEAP_DOMAIN=etdofresh.com
NAMECHEAP_HOST=workspaces
NAMECHEAP_DDNS_PASSWORD=your_ddns_password_here

# Existing MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your_secure_password_here
MINIO_DOMAIN=workspaces.etdofresh.com
MINIO_BROWSER_REDIRECT_URL=https://workspaces.etdofresh.com/minio/ui

# S3 Configuration
S3_ENDPOINT=http://minio:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=your_secure_password_here
S3_BUCKET=workspaces
S3_REGION=us-east-1

# Optional: Cloudflare R2 Configuration (if using)
R2_ACCOUNT_ID=your_account_id
R2_ACCESS_KEY_ID=your_access_key
R2_SECRET_ACCESS_KEY=your_secret_key
R2_BUCKET_NAME=your_bucket_name